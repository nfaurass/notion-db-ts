{
  "name": "notion-db-ts",
  "version": "1.0.2",
  "description": "A lightweight TypeScript ORM designed for Notion.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "dev": "ts-node src/NotionDB.ts",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfaurass/notion-db-ts.git"
  },
  "keywords": [
    "notion",
    "typescript",
    "orm",
    "lightweight"
  ],
  "author": "nfaurass",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfaurass/notion-db-ts/issues"
  },
  "homepage": "https://github.com/nfaurass/notion-db-ts#readme",
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "eslint": "^9.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
